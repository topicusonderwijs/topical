{
  "version": "2",
  "services": {
    "backend": {
      "scale": 1,
      "start_on_create": true,
      "health_check": {
        "response_timeout": 2000,
        "port": 80,
        "initializing_timeout": 600000,
        "interval": 2000,
        "strategy": "recreate"
      },
      "labels": {
        "io.rancher.container.pull_image": "always"
      }
    },
    "frontend": {
      "scale": 1,
      "start_on_create": true,
      "health_check": {
        "response_timeout": 2000,
        "port": 80,
        "initializing_timeout": 600000,
        "interval": 2000,
        "strategy": "recreate"
      },
      "labels": {
        "io.rancher.container.pull_image": "always"
      }
    }
  }
}